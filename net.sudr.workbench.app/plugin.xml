<?xml version="1.0" encoding="UTF-8"?>
<plugin>

   <extension
         id="product"
         point="org.eclipse.core.runtime.products">
      <product
            name="%product.workbench"
            application="org.eclipse.e4.ui.workbench.swt.E4Application">
         <property
               name="appName"
               value="%product.workbench">
         </property>
         <property
               name="applicationXMI"
               value="net.sudr.workbench.app/Application.e4xmi">
         </property>
         <property
               name="preferenceCustomization"
               value="workbench">
         </property>
         <property
               name="applicationCSS"
               value="platform:/plugin/net.sudr.workbench.app/css/default.css">
         </property>
      </product>
   </extension>

</plugin>
